{
    "name": "microsoft/kiota-serialization-form",
    "description": "Form implementation of Kiota abstractions serialization.",
    "version": "1.1.1",
    "authors": [
        {
            "name": "Microsoft Graph Client Tooling",
            "email": "graphtooling@service.microsoft.com",
            "role": "Developer"
        }
    ],
    "license": ["MIT"],
    "autoload": {
        "psr-4": {
            "Microsoft\\Kiota\\Serialization\\Form\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Microsoft\\Kiota\\Serialization\\Tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../abstractions",
            "options": {
                    "symlink": false
            }
        }
    ],
    "require": {
        "microsoft/kiota-abstractions": "^1.3.1",
        "guzzlehttp/psr7": "^1.6 || ^2",
        "php": "^7.4 || ^8",
        "ext-json": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^8 || ^9",
        "phpstan/phpstan": "^1.4"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
